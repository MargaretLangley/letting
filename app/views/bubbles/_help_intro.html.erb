<div id='help_0'  class='hide  bubble-intro  Bubble-main'>
  <p>
    <h3 class='help-heading'>&nbsp; &nbsp;
      <i class='fa fa-info-circle fa-lg'></i>
      <%= heading %>
    </h3>
  </p>

  <div class='text-in-box'>
    <% lines.each do |lines| %>
        <p><%= lines %></p>
    <% end %>
  </div>

  <div class='icons-in-box'>

    <p><i class='fa fa-print fa-lg  ButtonIcon-inactive'></i>&nbsp; Prints page</p>

    <% ikon.each do |ikon| %>
        <p>
          <i class='<%= ikon %> fa-lg  help-icon'></i>
        </p>
    <% end %>

  </div>

  <p class='text-in-box'>
    Using the next button in this box will point you to the different buttons on this page and their usage.
  </p>
  <div class = 'buttons-in-box'>
    <%= render 'bubbles/box' %>
  </div>
</div>
